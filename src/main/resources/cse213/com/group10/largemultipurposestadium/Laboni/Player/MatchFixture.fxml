          <TableColumn fx:id="dateColumn" text="Date" />
            <TableColumn fx:id="opponentColumn" prefWidth="96.0" text="Opponent" />
            <TableColumn fx:id="timeColumn" prefWidth="112.0" text="Time" />
            <TableColumn fx:id="venueColumn" prefWidth="123.0" text="Venue" />
